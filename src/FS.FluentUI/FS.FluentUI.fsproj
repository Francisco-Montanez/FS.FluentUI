<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Version>0.0.1-beta10</Version>
        <Authors>Andrew Sutton</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Description>F# bindings for the FluentUI library.</Description>
        <PackageProjectUrl>https://github.com/sydsutton/FS.FluentUI</PackageProjectUrl>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageTags>F# FSharp FS FluentUI Feliz Fable dotnet .NET</PackageTags>
        <PackageReleaseNotes>
            - Fix bugs in Fui.datePicker and related types
        </PackageReleaseNotes>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\..\README.md" Pack="true" PackagePath="\"/>
        <Compile Include="Domain.fs" />
        <Compile Include="Utils.fs" />
        <Compile Include="FelizProps.fs" />
        <Compile Include="Props.fs" />
        <Compile Include="FluentUI.fs" />
    </ItemGroup>

    <ItemGroup>
        <Content Include="*.fsproj; *.fs; *.js" Exclude="**\*.fs.js" PackagePath="fable\" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Update="FSharp.Core" Version="6.0.1" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Feliz" Version="2.6.0" />
    </ItemGroup>

    <PropertyGroup>
        <NpmDependencies>
            <NpmPackage Name="@fluentui/react-components" Version="&gt;= 9.3.0" ResolutionStrategy="Max" />
            <NpmPackage Name="@fluentui/react-datepicker-compat" Version="&gt;= 0.3.5" ResolutionStrategy="Max" />
            <NpmPackage Name="@fluentui/react-breadcrumb-preview" Version="&gt;= 0.3.8" ResolutionStrategy="Max" />
            <NpmPackage Name="@fluentui/react-search-preview" Version="&gt;= 0.1.15" ResolutionStrategy="Max" />
            <NpmPackage Name="@fluentui/react-tags-preview" Version="&gt;= 0.2.8" ResolutionStrategy="Max" />
        </NpmDependencies>
    </PropertyGroup>

</Project>
