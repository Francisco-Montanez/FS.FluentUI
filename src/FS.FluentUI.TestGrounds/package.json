    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts2fable": "^0.7.1",
    "use-sync-external-store": "^1.2.0"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "remotedev": "^0.2.9",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
